{
  "name": "video-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:html": "html-validate \"**/*.html\"",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:html",
    "test:perf": "lhci autorun --collect.staticDistDir=dist --upload.target=temporary-public-storage"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "eslint": "^9.0.0",
    "html-validate": "^8.0.0",
    "rimraf": "^6.0.0",
    "stylelint": "^16.0.0",
    "vite": "^5.0.0"
  },
  "license": "Apache-2.0"
}
